<%= simple_form_for[@list, @bookmark] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :comment %>
    <%= f.association :movie %>
    <%= f.association :list %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Create Bookmark" %>
  </div>
<% end %>
